{
	"name": "@natlibfi/express-validate-content-type",
	"description": "Express.js middleware for validating content type ",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://www.npmjs.com/package/@natlibfi/express-validate-content-type",
	"bugs": {
		"url": "https://github.com/natlibfi/express-validate-content-type-js/issues"
	},
	"repository": {
		"url": "https://github.com/NatLibFi/express-validate-content-type-js"
	},
	"license": "MIT",
	"version": "3.0.0-alpha.8",
	"main": "./dist/index.js",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22"
	},
	"scripts": {
		"prepare": "npm run build",
		"build": "esbuild \"src/**/*.js\" --sourcemap --platform=node --outdir=dist",
		"test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/**/*.test.js'",
		"dev:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/**/*.test.js'",
		"lint": "eslint --fix src",
		"test": "npm run lint && npm run test:base"
	},
	"devDependencies": {
		"cross-env": "^10.1.0",
		"esbuild": "^0.25.12",
		"eslint": "^9.39.1",
		"express": "^5.1.0"
	}
}
