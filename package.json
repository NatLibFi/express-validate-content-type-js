{
	"name": "@natlibfi/express-validate-content-type",
	"description": "Express.js middleware for validating content type ",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/express-validate-content-type-js",
	"bugs": {
		"url": "https://github.com/natlibfi/express-validate-content-type-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/express-validate-content-type-js.git"
	},
	"license": "MIT",
	"version": "3.0.0-alpha.3",
	"main": "./dist/index.js",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22"
	},
	"scripts": {
		"prepare": "npm run build",
		"test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
		"dev:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
		"lint": "eslint --fix src",
		"test": "npm run lint && npm run test:base"
	},
	"old_scripts": {
		"test:base": "cross-env NODE_ENV=test mocha --require @babel/register",
		"watch": "cross-env NODE_ENV=test chokidar src -c 'npm test'"
	},
	"dependencies": {
		"esbuild": "^0.25.9"
	},
	"devDependencies": {
		"cross-env": "^10.0.0",
		"eslint": "^9.34.0",
		"express": "^4.21.2"
	}
}
